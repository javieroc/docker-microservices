{
    "openapi": "3.0.0",
    "info": {
        "title": "User service API",
        "version": "0.1"
    },
    "paths": {
        "/api/login": {
            "post": {
                "operationId": "App\\Http\\Controllers\\AuthController::login",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "email": "test@test.test",
                                    "password": "12345"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "JWT"
                    },
                    "400": {
                        "description": "Email or Password is wrong"
                    }
                }
            }
        }
    }
}